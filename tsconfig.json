{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
		"moduleResolution": "Bundler",
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
		"lib": ["ES2015", "DOM"]
  },
	"include": ["src/**/*.js", "src/**/*.ts"]
}
